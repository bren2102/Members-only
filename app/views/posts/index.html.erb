<h1>Club House</h1>
<%= flash[:welcome] %>
<% unless logged_in? %>
  <p id="mkt-suggestion"> <a href="/login"> Login to see who wrote the post! </a></p> 
<% end %>
<% @posts.each do |x| %>
<h2><%=  x.title %></h2>
  <% if logged_in? %>
    <h4>Author: <%= x.user.name %></h4>
  <% end %>
  <p><%= x.content %></p>
<% end %>